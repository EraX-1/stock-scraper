{
  "name": "stock-scraper",
  "version": "1.0.0",
  "description": "Stock scraper with auto-login and MHTML capture using Playwright",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/containerMain.js",
    "login": "tsx src/loginSeparate.ts",
    "scrape": "tsx src/improvedScraper.ts",
    "scrape-all": "tsx src/fullUpdateScraper.ts",
    "auto-sync": "tsx src/autoSync.ts",
    "container": "tsx src/containerMain.ts",
    "main-job": "tsx src/mainJob.ts",
    "main-job:full": "tsx src/mainJob.ts --full",
    "main-job:azure": "tsx src/mainJob.ts --azure",
    "main-job:rag": "tsx src/mainJob.ts --rag",
    "main-job:full-azure": "tsx src/mainJob.ts --full --azure",
    "main-job:full-rag": "tsx src/mainJob.ts --full --rag",
    "main-job:all": "tsx src/mainJob.ts --azure --rag",
    "main-job:full-all": "tsx src/mainJob.ts --full --azure --rag",
    "job-stats": "tsx src/mainJob.ts --stats",
    "deploy-azure": "tsx src/azureDeploy.ts",
    "deploy-azure:dry": "tsx src/azureDeploy.ts --dry-run",
    "deploy-azure:list": "tsx src/azureDeploy.ts --list",
    "deploy-azure:timestamp": "tsx src/azureDeploy.ts --timestamp",
    "test-azure": "tsx src/storage/azureBlobManager.ts",
    "rag-index": "tsx src/ragIndexer.ts",
    "rag-index:blob": "tsx src/ragIndexer.ts --blob",
    "rag-index:batch10": "tsx src/ragIndexer.ts --batch=10",
    "rag-index:batch5": "tsx src/ragIndexer.ts --batch=5",
    "blob-index": "tsx src/stockBlobIndexer.ts",
    "blob-index:dry": "tsx src/stockBlobIndexer.ts --dry-run",
    "blob-index:slow": "tsx src/stockBlobIndexer.ts --concurrency 1 --delay 2000",
    "blob-index:fast": "tsx src/stockBlobIndexer.ts --concurrency 5 --delay 200",
    "blob-index:turbo": "tsx src/stockBlobIndexer.ts --concurrency 10 --delay 100",
    "cleanup": "tsx src/cleanup.ts",
    "cleanup:stats": "tsx src/cleanup.ts --stats",
    "cleanup:sessions": "tsx src/cleanup.ts --sessions",
    "cleanup:mhtml": "tsx src/cleanup.ts --mhtml"
  },
  "keywords": [
    "scraper",
    "stock",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@playwright/browser-chromium": "^1.54.1",
    "playwright": "^1.54.1",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}